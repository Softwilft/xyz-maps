{
  "name": "XYZ Maps Documentation",
  "out": "docs",
  "theme": "default",
  "exclude": "*.spec.ts",
  "disableSources": "true",
  "excludePrivate": "true",
  "excludeExternals": "true",
  "excludeNotDocumented": "true",
  "external-modulemap": ".*packages\/([^\/]+)\/.*",
  "plugin": ["@strictsoftware/typedoc-plugin-monorepo","typedoc-plugin-remove-references"]
}
